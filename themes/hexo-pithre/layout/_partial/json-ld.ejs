<% if(is_post()) { %>
<script type="application/ld+json">
  {
    "@context": "http://schema.org",
    "@type": "BlogPosting",
    "mainEntityOfPage": "<%= data.permalink %>",
    "headline": "<%= data.title %>",
    "datePublished": "<%= data.date.format() %>",
    "dateModified": "<%= data.updated.format() %>",
    "description": "<%= data.description %>",
    "author": {
      "@type": "Person",
      "name": "bravokeyl",
      "url": "https://bravokeyl.com",
      "sameAs": [
        "http://www.facebook.com/bravokeyl",
        "http://instagram.com/bravokeyl",
        "http://www.linkedin.com/in/bravokeyl",
        "http://plus.google.com/+bravokeyl"
      ]
    },
    "image": {
      "@type": "ImageObject",
      "url": "<%= config.url + image %>",
      "height": 300,
      "width": 700
    },
    "publisher": {
      "@type": "Organization",
      "name": "Mu Bit",
      "logo": {
        "@type": "ImageObject",
        "url": "https://bravokeyl.com/mu-bit.png",
        "width": 60,
        "height": 60
      }
    }
  }
</script>
<script type="application/ld+json">
{
  "@context": "http://schema.org",
  "@type": "BreadcrumbList",
  "itemListElement": [{
    "@type": "ListItem",
    "position": 1,
    "item": {
      "@id": "<%= config.url %>/wordpress",
      "name": "WordPress",
      "image": "<%= config.url %>/images/wordpress.png"
    }
  },{
    "@type": "ListItem",
    "position": 2,
    "item": {
      "@id": "<%= config.url %>/wordpress/genesis",
      "name": "Genesis",
      "image": "<%= config.url %>/images/genesis.png"
    }
  }
  ]
}
</script>
<% } else { %>
  <script type="application/ld+json">
    {
      "@context": "http://schema.org",
      "@type": "Website",
      "url": "https://bravokeyl.com/",
      "name": "Bravokeyl",
      "alternateName": "Double Pithre"
    }
  </script>
<% } %>
