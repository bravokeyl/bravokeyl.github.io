<header class="site-header">
  <div class="container">
    <div class="row">
      <div class="col-md-6 site-logo">
        <% if(is_home()){ %>
          <h1 class="site-title">
            <a href="/" class="logo">bravokeyl</a>
          </h1>
        <% } else { %>
            <a href="/" class="logo">bravokeyl</a>
        <% } %>
      </div>
      <div class="col-md-6 site-menu">
        <nav>
          <ul>
            <% for (var i in theme.menu) { %>
                <li>
                    <a href="<%- url_for(theme.menu[i]) %>"><%= i %></a>
                </li>
            <% } %>
          </ul>
        </nav>
      </div>
    </div>
  </div>
</header>
