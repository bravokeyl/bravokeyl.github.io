<header class="site-header">
  <div class="container">
    <div class="row">
      <div class="col-md-6 col-xs-12 site-logo">
        <a href="/" class="current-parent logo">bravokeyl</a>
      </div>
      <div class="col-md-6 col-xs-12 site-menu">
        <nav>
          <ul>
            <% for (var i in theme.menu) { %>
                <li>
                    <a href="<%- url_for(theme.menu[i]) %>"><%= i %></a>
                </li>
            <% } %>
          </ul>
        </nav>
      </div>
    </div>
  </div>
</header>
